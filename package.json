{
  "name": "self-sigma",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/generate-icons.mjs",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.vue",
    "format": "prettier --write .",
    "deploy": "npm run build && netlify deploy --prod --dir dist",
    "deploy:preview": "npm run build && netlify deploy --dir dist",
    "update-version": "node scripts/update-version.mjs",
    "release": "npm run update-version && git add . && git commit -m \"Release v$(node -p \"require('./package.json').version\")\" && git push"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "chart.js": "^4.4.3",
    "html2pdf.js": "^0.10.1",
    "vue": "^2.7.16"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^2.3.3",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.28.0",
    "prettier": "^3.3.3",
    "sharp": "^0.33.4",
    "vite": "^5.4.0",
    "vite-plugin-pwa": "^0.20.5",
    "vue-template-compiler": "^2.7.16"
  }
}
